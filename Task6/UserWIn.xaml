        <Label Content="Stanje" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="34,113,0,0"/>
        <TextBox x:Name="State" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="151,117,0,0"/>
        <Label Content="Broj uplata" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="34,154,0,0"/>
        <TextBox x:Name="RecNum" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="151,158,0,0"/>
        <Label Content="Broj isplata" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="34,186,0,0"/>
        <TextBox x:Name="PayNum" HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="151,190,0,0"/>

    </Grid>
</Window>
